      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!usuario.resolver.spec.ts",
      "!menus.resolver.spec.ts",
      "!roles.service.spec.ts",
      "!microservicios.resolver.spec.ts"
